<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="thirdDotThree.MainController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <TableView fx:id="table" prefHeight="229.0" prefWidth="300.0">
      <columns>
          <TableColumn prefWidth="75.0" text="C1"/>
          <TableColumn prefWidth="75.0" text="C2"/>
          <TableColumn prefWidth="75.0" text="C3"/>
          <TableColumn prefWidth="75.0" text="C4"/>
          <TableColumn prefWidth="75.0" text="C5"/>
          <TableColumn prefWidth="75.0" text="C6"/>
      </columns>
        <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
    <Button fx:id="mainLineBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
            text="Отразить по главной диагонали">
        <VBox.margin>
            <Insets top="10.0"/>
        </VBox.margin>
    </Button>
    <Button fx:id="secondLineBtn" layoutX="20.0" layoutY="255.0" maxWidth="1.7976931348623157E308"
            mnemonicParsing="false" text="Отразить по побочной диагонали">
        <VBox.margin>
            <Insets top="10.0"/>
        </VBox.margin>
    </Button>
    <Button fx:id="firstToLastBtn" layoutX="20.0" layoutY="305.0" maxWidth="1.7976931348623157E308"
            mnemonicParsing="false" text="Поменять местами первую и последную строку">
        <VBox.margin>
            <Insets top="10.0"/>
        </VBox.margin>
    </Button>
</VBox>
